: '

echo "
#!/bin/sh
PREREQ=\"\"
prereqs()
{
        echo "$PREREQ"
}
case $1 in
prereqs)
        prereqs
        exit 0
        ;;
esac
"
exit 0
'

python3 helper.py /bin binprofile
python3 helper.py /etc binprofile
python3 helper.py /lib binprofile

python3 helper.py /sbin sbinprofile

python3 helper.py /usr/bin usrbinprofile
python3 helper.py /usr/lib usrlibprofile
python3 helper.py /usr/sbin usrsbinprofile
python3 helper.py /usr/share usrsbinprofile
python3 helper.py /opt usrsbinprofile
