: '

echo "
#!/bin/sh
PREREQ=\"\"
prereqs()
{
        echo "$PREREQ"
}
case $1 in
prereqs)
        prereqs
        exit 0
        ;;
esac
"
exit 0
'

python3 defaultprofilegenerator_helper.py /bin 
python3 defaultprofilegenerator_helper.py /etc 
python3 defaultprofilegenerator_helper.py /lib 

python3 defaultprofilegenerator_helper.py /sbin 

python3 defaultprofilegenerator_helper.py /usr/bin 
python3 defaultprofilegenerator_helper.py /usr/lib 
python3 defaultprofilegenerator_helper.py /usr/sbin 
python3 defaultprofilegenerator_helper.py /usr/share 
python3 defaultprofilegenerator_helper.py /opt 
