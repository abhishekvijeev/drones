		/etc/** rwpix,
		/home/abhishek/ r,
		/home/abhishek/.*/** rwk,
		/home/abhishek/** rwkpix,
	
		/lib/** rmpix,
		/opt/** rmpix,
		/proc/ r,
		/proc/[0-9]*/oom_score_adj w,
		/proc/[0-9]*/** rw,
		/proc/sys/** rw,
		/proc/mtrr w,
		/proc/* r,
		/run/** rwkpix,
		

		/sbin/* rpix,
		/snap/** rmpix,
		/sys/bus/** rw,
		/sys/** r,
		/usr/** rmpix,
		/var/** rwlkpix,
		/tmp/ r,
		/tmp/** rwpix,	
		
	}
	
	profile usrsbinprofile {
		capability,
		ptrace,
		mount,
		signal,
		/ r,
		/boot/** rwl,	
		/bin/ r,
		/bin/* rix,
		/dev/ r,		
		/dev/null rw,
		/dev/** rwk,
		/etc/ r,		
		/etc/** rwkix,
		/home/ r,
		/home/abhishek/ r,
		/home/abhishek/.*/** rwlk,
		/home/abhishek/** rwmlkpix,
		/lib32/ld-2.29.so rix,
		/lib/ r,
		/lib/** rmix,
		/opt/** rmix,
		/proc/ r,
		/proc/[0-9]*/** rw,
		/proc/* r,
		/proc/sys/** rw,
		
		/run/** rwkix,
		/sbin/ r,
		/sbin/apparmor_parser rux,
		/sbin/* rix,
		/snap/** rmix,
		/sys/bus/** rw,
		/sys/** r,
		/usr/** rmix,
		/var/** rwlkix,
		/tmp/ r,
		/tmp/** rwix,	
		
	}
	
	profile usrlibprofile {
		capability,
		ptrace,
		mount,
		umount,
		signal,
		/bin/ r,
		/bin/* rpix,
		/dev/ r,		
		/dev/bus/usb/ r,
		/dev/null wr,
		/dev/** rwk,
		
		/etc/ r,
		/etc/** rwpix,
		/home/ r,
		/home/abhishek/ r,
		/home/abhishek/.*/** rwk,		
		/home/abhishek/** rwkpix,
		
		/lib/** rmpix,
		/opt/** rmpix,
		/proc/ r,
		/proc/[0-9]*/** rw,
		/proc/* r,
		/proc/sys/** rw,
				
		/run/** rwkpix,
		/sbin/ r,
		/sbin/apparmor_parser rux,
		/sbin/* rpix,
		/snap/** rmpix,
		/sys/bus/** rw,
		/sys/** r,
		/usr/** rmpix,
		/var/** rwkpix,
		/tmp/ r,		
		/tmp/** rwpix,		
		
	}



}"

mount -t securityfs none /sys/kernel/security
