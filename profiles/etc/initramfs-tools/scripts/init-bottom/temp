		/sys/** r,
		/swapfile r,
		/swapfile wr,
		/run/** rwkpix,
		/usr/** rmpix,
		/var/** rwk,
		/tmp/** rwpix,	
		
	}
	
	profile usrbinprofile {
		capability,
		ptrace,
		mount,
		signal,
		
		/bin/* rpix,
		/boot/grub/* rwpix,		
		/dev/ r,		
